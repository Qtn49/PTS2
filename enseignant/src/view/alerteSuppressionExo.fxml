<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <TitledPane animated="false" collapsible="false" expanded="false" text="Suppression d'un exercice" BorderPane.alignment="CENTER" />
   </top>
   <center>
      <DialogPane BorderPane.alignment="CENTER">
         <content>
            <Label text="Êtes-vous sûr de vouloir supprimer ?" />
         </content>
      </DialogPane>
   </center>
   <bottom>
      <HBox alignment="CENTER" spacing="60.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" text="Annuler" />
            <Button mnemonicParsing="false" text="Confirmer" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
      </HBox>
   </bottom>
</BorderPane>
